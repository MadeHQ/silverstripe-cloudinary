{
    "name": "mademedia/silverstripe-cloudinary",
    "type": "silverstripe-vendormodule",
    "description": "Adds the Cloudinary browser to the CMS.",
    "homepage": "https://github.com/MadeHQ/silverstripe-cloudinary",
    "keywords": ["silverstripe", "cloudinary", "mademedia", "madehq"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Made Media Ltd.",
            "role": "Developer",
            "email": "developers@made.media",
            "homepage": "https://made.media"
        },
        {
            "name": "Bartlomiej Bialek",
            "role": "Developer",
            "email": "bartlomiej.bialek@made.media"
        },
        {
            "name": "Lee Bradley",
            "role": "Developer",
            "email": "lee.bradley@made.media"
        },
        {
            "name": "Daniel James",
            "role": "Developer",
            "email": "daniel.james@made.media"
        },
        {
            "name": "Hiren Patel",
            "role": "Developer",
            "email": "hiren.patel@made.media"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "cloudinary/cloudinary_php": "^1.14",
        "silverstripe/framework": "^4.3@dev"
    },
    "autoload": {
        "psr-4": {
            "MadeHQ\\Cloudinary\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ],
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "discard-changes": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
