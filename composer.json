{
  "name": "mademedia/silverstripe-cloudinary",
  "type": "silverstripe-vendormodule",
  "description": "Fields & DataObjects to add Cloudinary Images/Files to DataObjects.",
  "homepage": "https://github.com/MadeHQ/silverstripe-cloudinary.git",
  "keywords": ["silverstripe","cloudinary"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Made Media",
      "role": "Developer",
      "email": "developers@mademedia.co.uk",
      "homepage": "https://www.example.com"
    }
  ],
  "require": {
    "cloudinary/cloudinary_php": "1.8.0",
    "php": ">=5.6.0",
    "silverstripe/framework": "^4@dev"
  },
  "autoload": {
    "psr-4": {
      "MadeHQ\\Cloudinary\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
