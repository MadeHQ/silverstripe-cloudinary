{
    "name": "silverstripe-cloudinary",
    "version": "2.0.0",
    "description": "Adds the Cloudinary browser to the CMS.",
    "homepage": "https://github.com/MadeHQ/silverstripe-cloudinary",
    "bin": {
        "webpack": "./node_modules/webpack/webpack.js"
    },
    "engines": {
        "node": "^6.x"
    },
    "config": {
        "paths": {
            "js": "client/src/js",
            "dist": "client/dist"
        }
    },
    "scripts": {
        "watch-js": "webpack --watch",
        "watch": "concurrently --kill-others 'npm run watch-js'",
        "build-js": "webpack --progress",
        "build": "export NODE_ENV=production && npm run build-js"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:MadeHQ/silverstripe-cloudinary.git"
    },
    "keywords": [
        "silverstripe",
        "cloudinary",
        "mademedia",
        "madehq"
    ],
    "author": "Made Media Ltd.",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/MadeHQ/silverstripe-cloudinary/issues"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "babel-loader": "^8.0.6",
        "babel-preset-env": "^1.7.0",
        "eslint": "^6.1.0",
        "path": "^0.12.7",
        "uglifyjs-webpack-plugin": "^2.1.3",
        "webpack": "^4.36.1",
        "webpack-cli": "^3.3.6"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    }
}
